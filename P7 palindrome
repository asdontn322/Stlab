#include<iostream.h>

#include<conio.h>

#include<string.h>

void main()

  {

    char*x,*y;

    int b;

    clrscr();

    cout<<"PALINDROME CHECKING\n";

    cout<<"\n Enter a string: ";

    cin>>x;

    strcpy(y,x);

    strrev(x);

    b = strcmp(x,y);

    if(b == 0)

      cout<<"\n The given string is a palindrome";

    else

      cout<<"\n The given string is not a palindrome";

    getch();

  }

